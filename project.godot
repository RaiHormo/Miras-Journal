; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Mira's Journal - Prototype"
config/description="A prototype JRPG game
Created by RaiDev"
config/version="0.0.3"
run/main_scene="res://codings/Initializer.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="miras-journal"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.4", "Mobile")
boot_splash/image="res://art/Branding/splash.png"
config/icon="res://art/Branding/icon.png"

[aseprite]

animation/storage/use_metadata=false
animation/storage/enable_metadata_removal_on_export=false
wizard/cleanup/automatically_hide_sprites_not_in_animation=false

[audio]

buses/default_bus_layout="res://sound/bus.tres"

[autoload]

Global="*res://codings/Autoload/Global.gd"
PartyUI="*res://codings/Autoload/PartyUI.tscn"
Loader="*res://codings/Autoload/Loader.tscn"
Item="*res://codings/Autoload/ItemManager.tscn"
World="*res://codings/Autoload/WorldEnvironment.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Event="*res://codings/Autoload/Event.tscn"

[debug]

settings/crash_handler/message="Looks like the Dev fucked up. If you are seeing this, please report it to them"
settings/crash_handler/message.editor="Looks like the Dev fucked up. If you are seeing this, please report it to them"
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/shadowed_variable_base_class=0
gdscript/warnings/shadowed_global_identifier=0
gdscript/warnings/integer_division=0
gdscript/warnings/int_as_enum_without_cast=0
gdscript/warnings/int_as_enum_without_match=0

[dialogue_manager]

runtime/state_autoload_shortcuts=["Global", "Loader", "World", "Event", "PartyUI", "Item"]
editor/wrap_long_lines=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/size/initial_position_type=2
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/SmoothScroll/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/mouse_coordinates/plugin.cfg", "res://addons/scene-tree-sorter/plugin.cfg")

[file_customization]

folder_colors={
"res://UI/": "green",
"res://addons/": "gray",
"res://art/": "pink",
"res://codings/": "blue",
"res://database/": "yellow",
"res://rooms/": "orange",
"res://sound/": "red"
}

[filesystem]

import/blender/enabled=false

[global_group]

TileMap=""

[gui]

common/snap_controls_to_pixels=false
fonts/dynamic_fonts/use_oversampling=false

[importer_defaults]

texture={
&"compress/channel_pack": 1,
&"detect_3d/compress_to": 2
}

